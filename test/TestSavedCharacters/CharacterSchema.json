{
	"$schema": "http://json-schema.org/schema#",
	"type": "object",
	"properties": {
		"ParameterData": {
			"type": "object",
			"properties": {
				"Name": {
					"type": "string"
				},
				"Age": {
					"type": "integer"
				},
				"Dead": {
					"type": "boolean"
				},
				"NullType": {
					"type": "null"
				},
				"Weight": {
					"type": "number"
				}
			},
			"required": [
				"Age",
				"Dead",
				"Name",
				"NullType",
				"Weight"
			]
		},
		"MixedArray": {
			"type": "array",
			"items": {
				"type": [
					"boolean",
					"null",
					"number",
					"string"
				]
			}
		},
		"Skills": {
			"type": "array",
			"items": {
				"type": "object",
				"properties": {
					"Hide": {
						"type": "integer"
					},
					"attributes": {
						"type": "object",
						"properties": {
							"AffectedByLight": {
								"type": "boolean"
							}
						},
						"required": [
							"AffectedByLight"
						]
					},
					"Sneak": {
						"type": "integer"
					},
					"Athletics": {
						"type": "integer"
					}
				}
			}
		}
	},
	"required": [
		"MixedArray",
		"ParameterData",
		"Skills"
	]
}